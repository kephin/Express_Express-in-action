<% include _header %>

<div class="container">
  <h1>Welcome to Account app</h1>
  <% users.forEach(user => { -%>
    <div class="panel panel-default">
      <div class="panel-heading">
        <a href="/users/<%= user.username %>">
          <%= user.name() %>
        </a>
      </div>
      <% if (user.bio) { -%>
        <div class="panel-body">
          <%= user.bio %>
        </div>
      <% } -%>
    </div>
  <% }) -%>
</div>

<% include _footer %>